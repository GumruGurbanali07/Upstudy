@using CourseApp.Models
@model ContactDetail
@addTagHelper*,Microsoft.AspNetCore.Mvc.TagHelpers
@{
    ViewData["Title"] = "Create Contact Detail";
    Layout = "~/Areas/Admin/Views/Shared/_adminLayout.cshtml";
}

<div class="col-lg-12 top20">
    <div class="widgets-container">


        <h5>Create Contact Detail</h5>
        <hr>
        <form method="post" enctype="multipart/form-data">
            <div class="row">
                <div class="col-md-4">
                    <input asp-for="Address" class="form-control" placeholder="Address" type="text">

                </div>
                <div class="col-md-4">
                    <input asp-for="PhoneNumber" class="form-control" placeholder="Phone number" type="text">

                </div>

            </div>
            <br />
            <div class="row">
                <div class="col-md-12">
                    <input asp-for="Email" class="form-control" placeholder="Email" type="email">

                </div>

            </div>
            <div class="row">
                <div class="col-md-12">
                    <input asp-for="Iframe" class="form-control" placeholder="Iframe" type="text">

                </div>

            </div>
            <br />
            <button class="btn btn-primary" type="submit">Save</button>
        </form>
    </div>
</div>

